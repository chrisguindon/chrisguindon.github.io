{
  "name": "chrisguindon.github.io",
  "version": "0.0.0",
  "description": "Christopher Guindon - Eclipse Foundation - Blog",
  "main": "package.json",
  "scripts": {
    "install": "yarn run fetch-theme",
    "fetch-theme": "rm -rf themes/hugo-cactus-theme && mkdir themes && git clone https://github.com/digitalcraftsman/hugo-cactus-theme.git themes/hugo-cactus-theme",
    "postinstall": "mkdir -p static/fonts && cp -rf themes/hugo-cactus-theme/static/fonts/* static/fonts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisguindon/chrisguindon.github.io.git"
  },
  "author": "Christopher Guindon",
  "license": "EPL-2.0",
  "bugs": {
    "url": "https://github.com/chrisguindon/chrisguindon.github.io/issues"
  },
  "homepage": "https://www.chrisguindon.com"
}
